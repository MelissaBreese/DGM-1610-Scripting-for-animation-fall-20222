using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      int x = 10;
      x += 5;
      Console.WriteLine(x);  
     }
  }
}



using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      int x = 3;
      int y = 5;
      Console.WriteLine(x != y);  // returns True because 5 is not equal to 3
     }
  }
}
